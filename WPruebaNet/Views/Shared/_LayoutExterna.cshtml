
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title> @ViewData["Title"]</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.css">
    <link rel="stylesheet" href="~/css/layoutExternal.css" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/sweetalert/js/sweetalert.js"></script>
    <script src="~/lib/fontawesome/js/all.js"></script>
</head>
<body>
    <header class="w-100">
        <div id="logo" class="p-2">
            <i class="fa-brands fa-edge-legacy"></i>duSoft
        </div>
    </header>
    <div class="container body-content mb-3 mt-3 h-75">
        @RenderBody()
    </div>
    <footer class="w-100">
        <p class="pt-4 text-center">Edusoft - Todos los derechos reservados @DateTime.Now.Year</p>
    </footer>
</body>
</html>